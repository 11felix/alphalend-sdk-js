{
  "name": "alphalend-sdk",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "prebuild": "npm run pretty && npm run lint",
    "build": "npm run build:cjs && npm run build:esm",
    "build:esm": "tsc --project tsconfig.esm.json && echo '{\"type\":\"module\"}' > dist/esm/package.json",
    "build:cjs": "tsc --project tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "postbuild": "npm link",
    "pretty": "prettier . --write",
    "lint": "eslint 'src/**/*.ts' --fix",
    "prettier": "prettier --write 'src/**/*.ts'",
    "gen": "node embed-graphql.js",
    "docs": "typedoc",
    "test:esm": "jest --config jest.config.esm.js",
    "test:cjs": "jest --config jest.config.cjs.js",
    "test": "npm run test:esm && npm run test:cjs",
    "prepublish": "npm run docs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@pythnetwork/pyth-sui-js": "^2.1.0",
    "bech32": "^2.0.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "babel-loader": "^10.0.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.14.0"
  },
  "peerDependencies": {
    "@mysten/dapp-kit": "^0.14.49",
    "@mysten/sui": "^1.21.1",
    "@mysten/wallet-standard": "^0.13.25"
  }
}
